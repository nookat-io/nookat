{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "Nookat",
  "version": "0.1.0",
  "identifier": "net.nookat",
  "build": {
    "beforeDevCommand": "npm run dev",
    "devUrl": "http://localhost:1420",
    "beforeBuildCommand": "npm run build",
    "frontendDist": "../dist"
  },
  "app": {
    "windows": [
      {
        "title": "nookat",
        "width": 800,
        "height": 600,
        "minWidth": 1280,
        "minHeight": 800,
        "decorations": true,
        "transparent": true
      }
    ],
    "security": {
      "csp": null
    },
    "macOSPrivateApi": true
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/128x128.png"
    ]
  }
}
