{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "Nookat",
  "version": "0.1.6",
  "identifier": "io.nookat",
  "build": {
    "beforeDevCommand": "npm run dev",
    "beforeBuildCommand": "npm run build",
    "devUrl": "http://localhost:1420",
    "frontendDist": "../dist"
  },
  "app": {
    "windows": [
      {
        "label": "main",
        "title": "Nookat",
        "width": 1280,
        "height": 800,
        "minWidth": 1280,
        "minHeight": 800,
        "decorations": true,
        "transparent": false,
        "titleBarStyle": "Overlay",
        "resizable": true,
        "center": true,
        "maximizable": true,
        "visible": false,
        "url": "/"
      },
      {
        "label": "splashscreen",
        "title": "Nookat - Starting...",
        "width": 400,
        "height": 300,
        "decorations": false,
        "transparent": true,
        "center": true,
        "visible": true,
        "resizable": false,
        "maximizable": false,
        "minimizable": false,
        "url": "/#/splashscreen"
      }
    ],
    "security": {
      "csp": null
    },
    "macOSPrivateApi": true
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}
